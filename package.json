{
	"name": "@ecopages/ecopages",
	"private": true,
	"version": "0.1.5",
	"scripts": {
		"watch:dev:playground": "bun run --filter @ecopages/playground watch:dev | tee /dev/null",
		"dev:playground": "bun run --filter @ecopages/playground dev | tee /dev/null",
		"build:playground": "bun run --filter @ecopages/playground build",
		"start:playground": "bun run --filter @ecopages/playground start",
		"preview:playground": "bun run --filter @ecopages/playground preview",
		"watch:dev:starter": "bun run --filter @ecopages/starter watch:dev | tee /dev/null",
		"dev:starter": "bun run --filter @ecopages/starter dev | tee /dev/null",
		"build:starter": "bun run --filter @ecopages/starter build",
		"start:starter": "bun run --filter @ecopages/starter start",
		"preview:starter": "bun run --filter @ecopages/starter preview",
		"dev:docs": "bun run --filter @ecopages/docs dev | tee /dev/null",
		"build:docs": "bun run --filter @ecopages/docs build",
		"start:docs": "bun run --filter @ecopages/docs start",
		"preview:docs": "bun run --filter @ecopages/docs preview",
		"dev:docs-radiant": "bun run --filter @ecopages/docs-radiant dev | tee /dev/null",
		"build:docs-radiant": "bun run --filter @ecopages/docs-radiant build",
		"start:docs-radiant": "bun run --filter @ecopages/docs-radiant start",
		"preview:docs-radiant": "bun run --filter @ecopages/docs-radiant preview",
		"dev:radiant-ui": "bun run --filter @ecopages/radiant-ui dev | tee /dev/null",
		"build:radiant-ui": "bun run --filter @ecopages/radiant-ui build",
		"start:radiant-ui": "bun run --filter @ecopages/radiant-ui start",
		"preview:radiant-ui": "bun run --filter @ecopages/radiant-ui preview",
		"watch:dev:react-app": "bun run --filter @ecopages/react-app watch:dev | tee /dev/null",
		"dev:react-app": "bun run --filter @ecopages/react-app dev | tee /dev/null",
		"build:react-app": "bun run --filter @ecopages/react-app build",
		"start:react-app": "bun run --filter @ecopages/react-app start",
		"preview:react-app": "bun run --filter @ecopages/react-app preview | tee /dev/null",
		"clean": "clear && rm -rf node_modules bun.lockb && bun install",
		"format": "bunx @biomejs/biome format --write .",
		"lint": "bunx @biomejs/biome check --apply .",
		"lint-staged": "lint-staged",
		"jsr:sync-version": "bun scripts/jsr-sync-version.ts",
		"prerelease": "bun run test:suite && bun run jsr:sync-version",
		"release": "bun run --filter '*' release:jsr | tee /dev/null",
		"test:suite": "bun run --filter '*' test | tee /dev/null"
	},
	"devDependencies": {
		"husky": "^9.0.10",
		"lint-staged": "^15.2.2"
	},
	"module": "src/index.js",
	"repository": "https://github.com/ecopages/ecopages.git",
	"workspaces": [
		"packages/*",
		"packages/integrations/*",
		"packages/loaders/*",
		"packages/plugins/*",
		"packages/processors/*",
		"examples/*",
		"apps/*"
	],
	"type": "module"
}
