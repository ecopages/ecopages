{
	"name": "@ecopages/ecopages",
	"private": true,
	"version": "0.1.60",
	"scripts": {
		"watch:dev:playground-global": "bun run --elide-lines 0 --filter @ecopages/playground-global watch:dev",
		"hot:dev:playground-global": "bun run --elide-lines 0 --filter @ecopages/playground-global hot:dev",
		"dev:playground-global": "bun run --elide-lines 0 --filter @ecopages/playground-global dev",
		"build:playground-global": "bun run --filter @ecopages/playground-global build",
		"start:playground-global": "bun run --filter @ecopages/playground-global start",
		"preview:playground-global": "bun run --elide-lines 0 --filter @ecopages/playground-global preview",
		"watch:dev:playground-namespace": "bun run --elide-lines 0 --filter @ecopages/playground-namespace watch:dev",
		"hot:dev:playground-namespace": "bun run --elide-lines 0 --filter @ecopages/playground-namespace hot:dev",
		"dev:playground-namespace": "bun run --elide-lines 0 --filter @ecopages/playground-namespace dev",
		"build:playground-namespace": "bun run --filter @ecopages/playground-namespace build",
		"start:playground-namespace": "bun run --filter @ecopages/playground-namespace start",
		"preview:playground-namespace": "bun run --elide-lines 0 --filter @ecopages/playground-namespace preview",
		"watch:dev:playground-react": "bun run --elide-lines 0 --filter @ecopages/playground-react watch:dev",
		"hot:dev:playground-react": "bun run --elide-lines 0 --filter @ecopages/playground-react hot:dev",
		"dev:playground-react": "bun run --elide-lines 0 --filter @ecopages/playground-react dev",
		"build:playground-react": "bun run --filter @ecopages/playground-react build",
		"start:playground-react": "bun run --filter @ecopages/playground-react start",
		"preview:playground-react": "bun run --elide-lines 0 --filter @ecopages/playground-react preview",
		"watch:dev:playground-react-router": "bun run --elide-lines 0 --filter @ecopages/playground-react-router watch:dev",
		"hot:dev:playground-react-router": "bun run --elide-lines 0 --filter @ecopages/playground-react-router hot:dev",
		"dev:playground-react-router": "bun run --elide-lines 0 --filter @ecopages/playground-react-router dev",
		"build:playground-react-router": "bun run --filter @ecopages/playground-react-router build",
		"start:playground-react-router": "bun run --filter @ecopages/playground-react-router start",
		"preview:playground-react-router": "bun run --elide-lines 0 --filter @ecopages/playground-react-router preview",
		"watch:dev:playground-browser-router": "bun run --elide-lines 0 --filter @ecopages/playground-browser-router watch:dev",
		"dev:playground-browser-router": "bun run --elide-lines 0 --filter @ecopages/playground-browser-router dev",
		"build:playground-browser-router": "bun run --filter @ecopages/playground-browser-router build",
		"start:playground-browser-router": "bun run --filter @ecopages/playground-browser-router start",
		"preview:playground-browser-router": "bun run --elide-lines 0 --filter @ecopages/playground-browser-router preview",
		"watch:dev:playground-tailwind-v4": "bun run --elide-lines 0 --filter @ecopages/playground-tailwind-v4 watch:dev",
		"hot:dev:playground-tailwind-v4": "bun run --elide-lines 0 --filter @ecopages/playground-tailwind-v4 hot:dev",
		"dev:playground-tailwind-v4": "bun run --elide-lines 0 --filter @ecopages/playground-tailwind-v4 dev",
		"build:playground-tailwind-v4": "bun run --filter @ecopages/playground-tailwind-v4 build",
		"start:playground-tailwind-v4": "bun run --filter @ecopages/playground-tailwind-v4 start",
		"preview:playground-tailwind-v4": "bun run --elide-lines 0 --filter @ecopages/playground-tailwind-v4 preview",
		"dev:docs": "bun run --elide-lines 0 --filter @ecopages/docs dev",
		"build:docs": "bun run --filter @ecopages/docs build",
		"start:docs": "bun run --filter @ecopages/docs start",
		"preview:docs": "bun run --filter @ecopages/docs preview",
		"clean": "clear && bunx npkill -D -y && rm -rf bun.lock && bun install",
		"format": "bunx prettier --write .",
		"lint": "bunx oxlint . --fix",
		"lint-staged": "lint-staged",
		"jsr:sync-version": "bun scripts/jsr-sync-version.ts",
		"prerelease": "bun test --coverage && bun run jsr:sync-version",
		"release": "bun run --filter '*' release:jsr",
		"bump:patch": "bun run scripts/bump-version.ts patch && bun run jsr:sync-version",
		"bump:minor": "bun run scripts/bump-version.ts minor && bun run jsr:sync-version",
		"bump:major": "bun run scripts/bump-version.ts major && bun run jsr:sync-version",
		"commit": "better-commits",
		"prepare:husky": "husky",
		"test:browser": "bunx vitest run && bunx playwright test",
		"test:e2e": "bunx playwright test",
		"test:e2e:ui": "bunx playwright test --ui",
		"typecheck": "bun run --elide-lines 0 --filter '*' typecheck"
	},
	"devDependencies": {
		"@biomejs/biome": "2.0.6",
		"@playwright/test": "^1.57.0",
		"@vitest/browser": "^4.0.16",
		"@vitest/browser-playwright": "^4.0.16",
		"better-commits": "^1.18.1",
		"husky": "^9.0.10",
		"lint-staged": "^15.2.2",
		"oxlint": "^1.32.0",
		"playwright": "^1.57.0",
		"prettier": "^3.7.4",
		"vitest": "^4.0.16"
	},
	"module": "src/index.js",
	"repository": "https://github.com/ecopages/ecopages.git",
	"workspaces": [
		"packages/*",
		"packages/core/__fixtures__/app",
		"packages/integrations/*",
		"packages/loaders/*",
		"packages/plugins/*",
		"packages/processors/*",
		"playground/*",
		"e2e/fixtures/*",
		"apps/*",
		"examples/*"
	],
	"type": "module"
}
