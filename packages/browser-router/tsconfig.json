{
	"compilerOptions": {
		"lib": ["ESNext", "DOM", "DOM.Iterable"],
		"target": "ESNext",
		"module": "Preserve",
		"moduleResolution": "bundler",
		"strict": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"declaration": true,
		"declarationMap": true,
		"outDir": "./dist",
		"allowImportingTsExtensions": true,
		"noEmit": true
	}
}
