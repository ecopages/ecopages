{
	"name": "@ecopages/core",
	"version": "0.1.27",
	"description": "Core package for Ecopages",
	"keywords": ["ecopages", "core", "ssg"],
	"license": "MIT",
	"main": "./src/index.ts",
	"type": "module",
	"files": ["src"],
	"bin": {
		"ecopages": "./src/bin/ecopages.ts"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ecopages/ecopages.git",
		"directory": "packages/core"
	},
	"scripts": {
		"release:jsr": "bunx jsr publish"
	},
	"dependencies": {
