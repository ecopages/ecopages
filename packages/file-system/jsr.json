{
	"$schema": "https://jsr.io/schema/config-file.v1.json",
	"name": "@ecopages/file-system",
	"version": "0.1.62",
	"exports": {
		".": "./src/index.ts"
	},
	"exclude": ["test", "**/*.test.ts"],
	"imports": {
		"bun": "npm:@types/bun@latest"
	},
	"publish": {
		"include": ["src/**/*.ts", "LICENSE", "README.md", "jsr.json", "package.json"],
